File
{
	Rename(,'r');
	DirMake(,'d');
	FormatDrive(,'f');
	UnmountDrive(,'u');
	Abort(,CH_SHIFT_ESC);
	Exit(,CH_ESC);
}

Edit
{
	Ed(,CH_SPACE);
	Copy(,CH_CTRLC);
	Paste(,CH_CTRLV);
	DeleteFile(,CH_CTRLY);
}

CDDVD
{
	DiskChange(,'c');
	MountISOC(,'i');
	MakeRedSeaISOC(,'m');
	MakeISO9660ISO(,'M');
	BurnISOFile(,'B');
}

Program
{
	Include(,0,0x3F0000003F);
	SystemInclude(,0,0x23F0000023F);
}
